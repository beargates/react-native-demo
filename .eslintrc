{
  "parser": "babel-eslint",
  "extends": [
    "eslint-config-airbnb"
  ],
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true,
    "mocha": true
  },
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "import/no-extraneous-dependencies": 0,
    "object-curly-spacing": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "comma-dangle": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/jsx-indent": [
      2,
      2
    ],
    "react/jsx-indent-props": [
      2,
      2
    ],
    "jsx-a11y/no-static-element-interactions": 0,
    "semi": 0,
    "arrow-parens": 0,
    "linebreak-style": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0,
    "no-use-before-define": 0,
    "no-case-declarations": 0,
    "no-underscore-dangle": 0
  },
  "globals": {
    "jest": true,
    "expect": true,
    "env": true,
    "mm": true,
    "UrlConst": true
  }
}
